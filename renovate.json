{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":semanticCommitsDisabled"
  ],
  "ignoreDeps": [
    "guava",
    "com.google.guava:guava",
    "com.google.code.gson:gson",
    "gson"
  ],
  "labels": ["Renovate"],
  "rebaseWhen": "conflicted",
  "schedule": ["on the first day of the month"]
}
